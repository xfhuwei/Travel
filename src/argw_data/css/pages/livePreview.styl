// 实时预览页
.live-preview-container {
  width 100%
  .nav-box {
    float left
    width 300px
    height 100%
    border-right 1px solid #cee0f0
    box-sizing: border-box
    // overflow-y auto;
    background #f7faff
    position relative
    .nav-top {
      padding-bottom 242px;
      height 100%
      box-sizing border-box
      .title {
        font-size 15px
        line-height 34px
        border-bottom 1px solid #cee0f0
        padding-left 15px
        background #ECF4FD
        background-image linear-gradient(to bottom, #fefeff, #eff6ff)
      }
      .list {
        >a {
          font-size 14px
          line-height 34px;
          padding 0 20px
          display block
          text-decoration none
          overflow hidden
          text-overflow ellipsis
          white-space nowrap
          transition: background-color .123s
          color #666666
          cursor pointer
          &:hover {
            color #0394f9
          }
          &.active {
            background $hoverColor2
            color #0394f9
          }
          >i {
            iconFn(16px, 3px, url('../img/livePreview/camera.png'))
          }
        }
      }

    }

    .nav-bottom {
      height 242px
      position absolute
      bottom 0
      left 0
      width 100%
      overflow hidden
      background #f7faff
      .tags {
        height 30px
        line-height 30px
        border-top 1px solid #cee0f0
        border-bottom 1px solid #cee0f0
        background #ECF4FD
        background-image linear-gradient(to bottom, #fefeff, #eff6ff)
        font-size 12px
        position relative
        .tags-icon {
          position absolute
          font-size 14px
          right 0
          top 0
          padding 5px 10px
          cursor pointer
        }
        > a {
          display inline-block
          line-height 24px
          padding 0 10px
          border-radius 3px
          cursor pointer
          &.active {
            color #ffffff
            background #55b0ff
          }
        }

      }
      .handler {
        height 38px
        border-bottom 1px solid #cee0f0
        text-align right
        >a {
          display inline-block
          width 28px
          height 28px
          //box-sizing border-box
          border 1px solid #dae6f9
          margin-top 4px
          margin-right 6px
          cursor pointer
          //background red\

        }
      }
      .table {
        li {
          clearFloat()
          span {
            float left
            padding-left 5px
            overflow hidden
            box-sizing border-box
            &.col1 {
              width 20%
              text-align center
            }
            &.col2 {
              width 50%
            }
            &.col3 {
              width 30%
              text-align center
            }
          }
        }
        .table_header {
          li {
            border-bottom 1px solid #cee0f0
            background #eff6ff
            line-height 30px
            span {
              font-weight bold
              color #333
            }
          }
        }
        .table_body {
          height 140px
          overflow-y: auto
          background #fff
          li {
            line-height (140/4)px
            &:nth-child(odd) {
              background #f7faff
            }
            &:hover,&.active {
              background $hoverColor2
            }
          }
        }
      }
      .xunhang {
        .handler {
          .bo {
            background-image url("../img/livePreview/list/bo_f.png")
            &:hover {
              background-image url("../img/livePreview/list/bo.png")
