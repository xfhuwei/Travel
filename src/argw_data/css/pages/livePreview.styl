            }
          }

        }
        .table {

        }
      }
      .yuntai {
        >ul {
          padding 20px 10px 5px 10px
          li{
            margin-bottom 5px
            clearFloat()
            a {
              float left
              display inline-block
              height 37px
              width 52px
              box-sizing border-box
              //background #f4f7fd
              //background-image linear-gradient(to bottom, #fdfeff, #eef2fb)
              //border 1px solid #c1d5e6
              border-radius 3px
              cursor pointer
              background url("../img/livePreview/yuntai/left_top.png") no-repeat center
              //background-size contain
              &.middle-a {
                margin 0 5px
              }
              &.right-a {
                margin-left 8px
                position relative
                left 2px
              }
              &.left_top {
                background-image url("../img/livePreview/yuntai/left_top.png")
                &:hover {
                  background-image url("../img/livePreview/yuntai/left_top_f.png")
                }
              }
              &.top {
