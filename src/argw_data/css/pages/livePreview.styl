              background-color #55B0FF
            }
          }
          .xiu {
            background-image url("../img/livePreview/list/xiu_f.png")
            &:hover {
              background-image url("../img/livePreview/list/xiu.png")
              background-color #55B0FF
            }
          }
          .shan {
            background-image url("../img/livePreview/list/shan_f.png")
            &:hover {
              background-image url("../img/livePreview/list/shan.png")
              background-color #55B0FF
            }
          }
        }
        .table {

        }
      }
      .yuweizi {
        .handler {
          .jia {
            background-image url("../img/livePreview/list/jia_f.png")
            &:hover {
              background-image url("../img/livePreview/list/jia.png")
              background-color #55B0FF
            }
          }
          .shang {
            background-image url("../img/livePreview/list/shang_f.png")
            &:hover {
              background-image url("../img/livePreview/list/shang.png")
