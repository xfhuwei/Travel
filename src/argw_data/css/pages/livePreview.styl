                }
              }
              &.right_bottom {
                background-image url("../img/livePreview/yuntai/right_bottom.png")
                &:hover {
                  background-image url("../img/livePreview/yuntai/right_bottom_f.png")
                }
              }
              &.shi {
                background-image url("../img/livePreview/yuntai/shi.png")
                &:hover {
                  background-image url("../img/livePreview/yuntai/shi_f.png")
                }
              }
              &.xu {
                background-image url("../img/livePreview/yuntai/xu.png")
                &:hover {
                  background-image url("../img/livePreview/yuntai/xu_f.png")
                }
              }
              &.qian {
